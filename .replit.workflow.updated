run = ["python", "run_dual_app.py"]
onBoot = ["echo", "Starting MCP Assessor Agent API..."]
language = "python3"

[env]
PYTHONPATH = "/home/runner/workspace"
PYTHONUNBUFFERED = "1"

[nix]
channel = "stable-22_11"

[languages.python3]
pattern = "**/*.py"
syntax = "python"

[languages.python3.languageServer]
start = ["pylsp"]

[deployment]
run = ["python", "run_dual_app.py"]
deploymentTarget = "cloudrun"